<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:id="@+id/historyLayout"
    android:layout_height="50dp">

    <ImageView
        android:id="@+id/search_icon_fix"
        android:layout_width="39dp"
        android:layout_height="39dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.545"
        app:srcCompat="@drawable/search_orange" />

    <ImageView
        android:id="@+id/ic_delete"
        android:layout_width="12dp"
        android:layout_height="12dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/tvHistoryDetail"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvHistoryDetail"
        app:srcCompat="@drawable/cross" />

    <TextView
        android:id="@+id/tvHistoryDetail"
        android:layout_width="277dp"
        android:layout_height="20dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:text="TextViewasdasdasdasdaasdasdasdadasdaasda"
        android:textAppearance="@style/LamoonRegular"
        android:textSize="17sp"
        app:layout_constraintBottom_toBottomOf="@+id/search_icon_fix"
        app:layout_constraintStart_toEndOf="@+id/search_icon_fix"
        app:layout_constraintTop_toTopOf="@+id/search_icon_fix"
        app:layout_constraintVertical_bias="0.473" />

</androidx.constraintlayout.widget.ConstraintLayout>