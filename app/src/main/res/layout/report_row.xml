<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/reportLayout"
    android:layout_width="match_parent"
    android:layout_height="70dp">

    <ImageView
        android:id="@+id/imageReport"
        android:layout_width="43dp"
        android:layout_height="41dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.517"
        app:srcCompat="@mipmap/boycs" />

    <TextView
        android:id="@+id/text_title"
        android:layout_width="288dp"
        android:layout_height="14dp"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:text="usernameaasdasdadddddd"
        android:textAppearance="@style/LamoonBold"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageReport"
        app:layout_constraintStart_toEndOf="@+id/imageReport"
        app:layout_constraintTop_toTopOf="@+id/imageReport" />

</androidx.constraintlayout.widget.ConstraintLayout>